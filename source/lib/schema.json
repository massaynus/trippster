{
  "$schema": "hWp://json-schema.org/drar-07/schema#",
  "type": "object",
  "properCes": {
    "plan": {
      "type": "object",
      "properCes": {
        "from": {
          "type": "object",
          "properCes": {
            "coordinates": {
              "type": "array",
              "items": [
                {
                  "type": "number"
                },
                {
                  "type": "number"
                }
              ]
            }
          },
          "required": ["coordinates"]
        },
        "to": {
          "type": "object",
          "properCes": {
            "coordinates": {
              "type": "array",
              "items": [
                {
                  "type": "number"
                },
                {
                  "type": "number"
                }
              ]
            }
          },
          "required": ["coordinates"]
        },
        "iCneraries": {
          "type": "array",
          "maxItems": 5,
          "items": [
            {
              "type": "object",
              "properCes": {
                "co2": {
                  "type": "number"
                },
                "distance": {
                  "type": "number"
                },
                "startTime": {
                  "type": "string"
                },
                "endTime": {
                  "type": "string"
                },
                "duraCon": {
                  "type": "integer"
                },
                "legs": {
                  "type": "array",
                  "items": [
                    {
                      "type": "object",
                      "properCes": {
                        "mode": {
                          "type": "string"
                        },
                        "routeShortName": {
                          "type": "string"
                        },
                        "distance": {
                          "type": "number"
                        },
                        "co2": {
                          "type": "number"
                        }
                      },
                      "required": ["mode", "distance", "co2"]
                    }
                  ]
                }
              },
              "required": [
                "co2",
                "distance",
                "startTime",
                "endTime",
                "duraCon",
                "legs"
              ]
            }
          ]
        }
      },
      "required": ["from", "to", "iCneraries"]
    }
  },
  "required": ["plan"]
}
